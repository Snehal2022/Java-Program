import java.util.ArrayList;
import java.util.List;
 
public class removeElement
{
    public static void main(String[] args)
    {
        List<Integer> arrList = new ArrayList<>();
 
        arrList.add(10);
        arrList.add(20);
        arrList.add(30);
        arrList.add(1);
        arrList.add(2);
     
        System.out.println(arrList);
 
        arrList.remove(1);
 
        arrList.remove(3);

        System.out.println(arrList);
    }
}
